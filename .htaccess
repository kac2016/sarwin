<ifModule mod_gzip.c>
mod_gzip_on Yes
mod_gzip_dechunk Yes
mod_gzip_item_include file .(html?|txt|css|js|php|pl)$
mod_gzip_item_include handler ^cgi-script$
mod_gzip_item_include mime ^text/.*
mod_gzip_item_include mime ^application/x-javascript.*
mod_gzip_item_exclude mime ^image/.*
mod_gzip_item_exclude rspheader ^Content-Encoding:.*gzip.*

  # Images
  ExpiresByType images/1.png "access plus 1 year"
  ExpiresByType images/2.png "access plus 1 year"
  ExpiresByType images/3.png "access plus 1 year"
  ExpiresByType images/4.png "access plus 1 year"
  ExpiresByType images/counselling-in-chennai-540x358.jpg "access plus 1 year"
  ExpiresByType images/face1.jpg "access plus 1 year"
  ExpiresByType images/face2.jpg "access plus 1 year"
  ExpiresByType images/face3.jpg "access plus 1 year"
  ExpiresByType images/face4.jpg "access plus 1 year"
  ExpiresByType images/untitled-design-122x122.jpg.jpg "access plus 1 year"
  ExpiresByType images/svg+xml "access plus 1 year"
  ExpiresByType image/x-icon "access plus 1 year"


  # CSS, JavaScript
  ExpiresByType bootstrap/css/bootstrap-grid.min.css "access plus 1 month"
  ExpiresByType bootstrap/css/bootstrap-reboot.min.css "access plus 1 month"
  ExpiresByType bootstrap/css/bootstrap.min.css "access plus 1 month"
  ExpiresByType bootstrap/js/bootstrap.min.js "access plus 1 month"
  ExpiresByType cookies-alert-plugin/cookies-alert-core.js "access plus 1 month"
  ExpiresByType bootstrap/css/cookies-alert-plugin/cookies-alert-script.js "access plus 1 month"
  ExpiresByType dropdown/css/style.css "access plus 1 month"
  ExpiresByType dropdown/js/script.min.js "access plus 1 month"
  ExpiresByType gdpr-plugin/gdpr-styles.css "access plus 1 month"
  ExpiresByType mobirise/css/mbr-additional.css "access plus 1 month"
  ExpiresByType smoothscroll/smooth-scroll.js "access plus 1 month"
  ExpiresByType socicon/css/styles.css "access plus 1 month"
  ExpiresByType theme/css/style.css "access plus 1 month"
  ExpiresByType tether/tether.min.js "access plus 1 month"
  ExpiresByType touch-swipe/jquery.touch-swipe.min.js "access plus 1 month"
  ExpiresByType viewportchecker/jquery.viewportchecker.js "access plus 1 month
  ExpiresByType web/assets/jquery/jquery.min.js "access plus 1 month"
